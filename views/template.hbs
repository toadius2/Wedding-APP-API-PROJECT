        }

        .logo.header {
            background-size: 156px 40px;
            margin: 12px 8px 0px 24px;
        }

        .logo.footer {
            background-size: 125px 32px;
            margin: 8px 8px 0px 24px;
        }

        .header span {
            color: #fff;
        }

        .headline-1 {
            font-size: 11px;
        }

        .header .headline-2 {
            font-size: 16px;
            margin-left: auto;
            margin-right: 24px;
        }

        .content {
            padding: 48px 60px;
            color: #fff;
            text-align: center;
        }

        .content span {
            font-size: 14px;
        }

        .footer {
            width: 100%;
            height: 300px;
        }

        .footer .top {
            display: flex;
            flex-direction: row;
            align-items: center;
            margin: 40px 0px 0px;
        }

        .footer .bottom {
            display: flex;
            flex-direction: row;
            align-items: center;
            margin: 16px 24px;
        }

        .footer span {
            color: #fff;
        }

        .bottom span {
            margin-right: 8px;
        }


        .copyright {
            display: flex;
            flex-direction: row;
            align-items: center;
        }

        .social-container {
            display: flex;
            flex-direction: row;
            align-items: center;
            margin-left: auto;
            margin-right: 24px;
        }

        .social-icon {
            margin: 8px;
        }

        .social-icon.facebook-icon {
            background: no-repeat url('data:image/png;base64,');
            width: 13px;
            height: 25px;
        }

        .social-icon.insta-icon {
            background: no-repeat url('data:image/png;base64,');
            width: 25px;
            height: 25px;
        }

        .seperator {
            height: 1px;
            background-color: #fff;
            margin: 8px 24px;
        }

        .link-container {
            display: flex;
            flex-direction: row;
            margin: 12px 24px;
        }

        .link-container a {
            color: #fff;
            text-decoration: none;
            font-size: 12px;
            margin-right: 16px;
        }

        @media screen and (max-width: 720px) {
            .header .headline-2 {
                display: none;
            }

            .footer .top {
                flex-direction: column;
                align-items: flex-start;
            }

            .social-container {
                margin-left: 24px;
            }
        }
    </style>
    <meta name="color-scheme" content="only">
    <title>{{title}}</title>
</head>

<body>
    <div class="header">
        <div class="logo header" style="width: 156px; height: 40px;"></div>
        <span class="headline-2">Your perfect wedding day starts here.</span>
    </div>
    <div class="content">
        <h2 style="font-weight: 500;">{{{subtitle}}}</h2>
        <span>{{{content_body}}}</span>
    </div>
    <div class="footer">
        <div class="top">
            <div class="copyright">
                <div class="logo footer" style="background-size: 125px 32px; width: 125px; height: 32px"></div>
                <span class="headline-1">&#169; Copyright 2020, All Rights Reserved. Nuvow LLC.</span>
            </div>
            <div class="social-container">
                <a href="https://www.facebook.com/nuvow/">
                    <div class="social-icon facebook-icon" style="background-size: 13px 25px;"></div>
                </a>
                <a href="https://www.instagram.com/nuvow/">
                    <div class="social-icon insta-icon" style="background-size: 25px 25px"></div>
                </a>
            </div>
        </div>
    </div>
</body>

</html>
